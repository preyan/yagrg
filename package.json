{
  "name": "yagrg",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc:build": "compodoc -p tsconfig.json -d documentation",
    "compodoc:serve": "compodoc -s",
    "release": "standard-version",
    "lint": "ng lint"
  },
  "lint-staged": {
    "*.{html,ts,js,json,scss,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@ngrx/effects": "^21.0.1",
    "@ngrx/router-store": "^21.0.1",
    "@ngrx/store": "^21.0.1",
    "@ngrx/store-devtools": "^21.0.1",
    "bootstrap": "^5.3.8",
    "marked": "^17.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@angular-eslint/builder": "21.2.0",
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@compodoc/compodoc": "^1.2.1",
    "@eslint/js": "^9.39.2",
    "@types/marked": "^6.0.0",
    "@types/node": "^25.2.0",
    "@vitest/coverage-v8": "4.0.18",
    "angular-eslint": "21.2.0",
    "commitlint": "^20.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "prettier": "^3.8.1",
    "standard-version": "^9.5.0",
    "stylelint": "^17.1.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.50.1",
    "vitest": "^4.0.8"
  }
}
